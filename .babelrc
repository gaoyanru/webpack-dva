{
    "presets": [
      [
        "@babel/preset-env", {
          "useBuiltIns": "usage",
          "corejs": "3",
          "targets": {
            "browsers": ["> 5%", "last 2 versions"]
          },
          "modules": false 
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      [
        "import", {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css" // `style: true` 会加载 less 文件
        }
      ],
      [
        "import",
        {
          "libraryName": "lodash",
          "libraryDirectory": "",
          "camel2DashComponentName": false  // default: true
        },
        "loadsh"
      ]
    ]
  }
  